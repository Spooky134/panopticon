name: "calibration_pipeline"
platform: "ensemble"
max_batch_size: 8


ensemble_scheduling {
  step [
    {
      model_name: "calibration_preprocessor"
      model_version: -1
      input_map {
        key: "input_data"
        value: "raw_calibration_data"
      }
      output_map {
        key: "processed_features"
        value: "calibration_features"
      }
    },
    {
      model_name: "profile_builder"
      model_version: -1
      input_map {
        key: "features"
        value: "calibration_features"
      }
      output_map {
        key: "profile_data"
        value: "raw_profile"
      }
    },
    {
      model_name: "profile_postprocessor"
      model_version: -1
      input_map {
        key: "profile"
        value: "raw_profile"
      }
      output_map {
        key: "final_profile"
        value: "calibration_profile"
      }
    }
  ]
}